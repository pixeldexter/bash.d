# -*- shell-script -*-
# ~/.bash_completion: executed to set up completions for interactive shells

_bashdir=${BASH_SOURCE%/*}/.bash.d/

source $_bashdir/helpers/source-dir
source-dir $_bashdir/completion.d
test   -e $_bashdir/../.bash_completion.local \
    -a -r $_bashdir/../.bash_completion.local \
    &&  . $_bashdir/../.bash_completion.local \

# other completions may be added here
