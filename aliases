# -*- shell-script -*-
# ~/.bash_aliases: executed to set up aliases for interactive shells

_bashdir=${BASH_SOURCE%/*}/.bash.d/

source $_bashdir/helpers/source-dir
source-dir $_bashdir/aliases.d
test   -e $_bashdir/../.bash_aliases.local \
    -a -r $_bashdir/../.bash_aliases.local \
    &&  . $_bashdir/../.bash_aliases.local \

# other completions may be added here
